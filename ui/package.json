{
    "name": "flow_insight",
    "version": "1.0.0",
    "description": "Flow insight, provides monitoring with timeline view for pipelines status powered by Vue.js",
    "author": "yinchen <yinchen@freewheel.tv>",
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --open --config webpack.dev.js --mock",
        "start": "npm run dev",
        "start:server": "webpack-dev-server --open --config webpack.dev.js",
        "unit": "jest --config test/unit/jest.conf.js --coverage",
        "e2e": "node test/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
        "lint:fix": "eslint --ext .jsx,.js app --fix",
        "build": "node build/build.js"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint --fix",
            "git add"
        ],
        "*.{js,jsx,json,md,scss,yaml,yml}": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.3.4",
        "antd": "^3.20.1",
        "babel-plugin-react-css-modules": "^5.2.1",
        "babel-polyfill": "^6.26.0",
        "es6-promise": "^4.2.8",
        "moment": "^2.24.0",
        "react": "^15.6.2",
        "react-dom": "^15.6.2",
        "react-highlight": "^0.12.0",
        "react-router": "3.0.2",
        "react-vis": "^1.11.7",
        "spark-ui": "2.27.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.5",
        "@babel/plugin-transform-react-jsx": "^7.3.0",
        "@babel/plugin-transform-runtime": "^7.3.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.4.4",
        "babel-jest": "^24.8.0",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^1.0.1",
        "css-loader": "^2.1.0",
        "cypress": "^3.2.0",
        "eslint": "^5.14.1",
        "eslint-config-prettier": "^4.0.0",
        "eslint-plugin-cypress": "^2.2.1",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-modula": "^1.5.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^3.0.0",
        "jest": "^24.8.0",
        "jsdom": "^15.1.1",
        "lint-staged": "^9.2.0",
        "mini-css-extract-plugin": "^0.5.0",
        "mocha": "^6.1.4",
        "mock-css-modules": "^1.0.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "prettier": "^1.16.4",
        "pretty-quick": "^1.11.1",
        "react-test-renderer": "^16.8.6",
        "start-server-and-test": "^1.9.0",
        "static-asset-package-manager": "1.10.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.10.1",
        "stylelint-config-css-modules": "^1.3.0",
        "stylelint-config-standard": "^18.2.0",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "underscore-template-loader": "^1.0.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.2.1",
        "write-file-webpack-plugin": "^4.5.0"
    }
}
